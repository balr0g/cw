disk "c1541"
	{
	info "C1541, GCR, 5.25 inch, 35 trks, 1 side, 17-21 sec/trk, 170 K"
	side_offset 84
	clock 14
	track_range 1 69 2
		{
		format "fill"
		optional yes
		write fill_length 16384
		}
	format "gcr_cbm"
	image "d64_noerror"
	track_step 2
	read
		{
		timeout 180
		postcomp_simple yes
		sync_length 10
		}
	write
		{
		timeout 300
		precomp
			{
			+0x0000 +0x0100 +0x0100
			-0x0100 +0x0000 +0x0000
			-0x0100 +0x0000 +0x0000
			}
		prolog_length 2048
		}
	track_range 0 32 2
		{
		bounds_new
			{
			0x0f00 0x1200 0x1c00
			0x1d00 0x2500 0x2f00
			0x3000 0x3800 0x4800
			}
		sectors 21
		}
	track_range 34 46 2
		{
		bounds_new
			{
			0x1100 0x1400 0x1f00
			0x2000 0x2900 0x3400
			0x3500 0x3e00 0x5000
			}
		sectors 19
		}
	track_range 48 58 2
		{
		bounds_new
			{
			0x1200 0x1500 0x2000
			0x2100 0x2b00 0x3700
			0x3800 0x4100 0x5000
			}
		sectors 18
		}
	track_range 60 68 2
		{
		bounds_new
			{
			0x1300 0x1600 0x2200
			0x2300 0x2d00 0x3900
			0x3a00 0x4400 0x5000
			}
		sectors 17
		}
	}

