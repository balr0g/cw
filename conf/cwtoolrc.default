# default cwtoolrc with updates and fixes from Jonathan Gevaryahu
# jgevaryahu(at)hotmail(dot)com
#
# Legend:
# =======
#
# \ = derived from above or other
# ? = missing format, feel free to create, test, and send to me
#
# Formats supported:
# ==================
#
# ***GENERIC***
#
# clear      - clears all tracks
# pattern_14 - writes a 14MHz pattern on all tracks 
# pattern_28 - "      " 28MHz " ...
# pattern_56 - "      " 56MHz " ...
# raw_14  - raw flux dump at 14mhz unsynced
#\raw_14i - raw flux dump at 14mhz synced to index hole
#\raw_dd  - same as raw_14
# raw_28  - raw flux dump at 28mhz unsynced
#\raw_28i - raw flux dump at 28mhz synced to index hole
#\raw_hd  - same as raw_28
# raw_56  - raw flux dump at 56mhz unsynced
#\raw_56i - raw flux dump at 56mhz synced to index hole
#\raw_ed  - same as raw_56
#
#
# ***COMMODORE***
#
# c1541      - commodore 1541 .d64 binary image with no error log 35 tracks
#\c1541_40   - commodore 1541 .d64 binary image with no error log 40 tracks
#\c1541_42   - commodore 1541 .d64 binary image with no error log 42 tracks
# c1541_d64  - commodore 1541 .d64 binary image with error log 35 tracks
#\c1541_40_d64 - commodore 1541 .d64 binary image with error log 40 tracks
#\c1541_42_d64 - commodore 1541 .d64 binary image with error log 42 tracks
# c1541_g64    - commodore 1541 .g64 GCR image 35 tracks
#\c1541_40_g64 - commodore 1541 .g64 GCR image 40 tracks
#\c1541_42_g64 - commodore 1541 .g64 GCR image 42 tracks
# c1541_300    - same as above but for a 300rpm drive
#\c1541_40_300 - ditto
#\c1541_42_300 - ditto
# c1541_d64_300    - ditto
#\c1541_40_d64_300 - ditto
#\c1541_42_d64_300 - ditto
# c1541_g64_300    - ditto
#\c1541_40_g64_300 - ditto
#\c1541_42_g64_300 - ditto
# c1571    - commodore 1571 .d71 binary image with no error log, 35 tracks DS
# c1571_40 - commodore 1571 .d71 binary image with no error log, 40 tracks DS
#?c1571_42 - commodore 1571 .d71 binary image with no error log, 42 tracks DS
#?c1571_d71 - commodore 1571 .d71 binary image with error log, 35 tracks DS
#?c1571_40_d71 - commodore 1571 .d71 binary image with error log, 40 tracks DS
#?c1571_42_d71 - commodore 1571 .d71 binary image with error log, 42 tracks DS
#?c1571_g71    - commodore 1571 .g71 GCR image 35 tracks DS
#?c1571_40_g71 - commodore 1571 .g71 GCR image 40 tracks DS
#?c1571_42_g71 - commodore 1571 .g71 GCR image 42 tracks DS
# c1581_dsdd - commodore 1581 3.5" .d81? image DS
#?c1581_raw  - some sort of raw format of the above? raw mfm? not really sure...
# amiga_dsdd_5.25 - amiga, 5.25"DSDD GCR
# amiga_dsdd - amiga, 3.5"DD GCR
# amiga_dshd - amiga, 3.5"HD GCR
#
#
# ***MS-DOS/NE765***
#
# msdos_ssdd_5.25 - MS-DOS 5.25"SSDD 180K
#\msdos_dsdd_5.25 - MS-DOS 5.25"DSDD 360K
# msdos_ssdd_8sector_5.25 - MS-DOS 5.25"SSDD 160K (needs work/calibration)
#\msdos_dsdd_8sector_5.25 - MS-DOS 5.25"DSDD 320K (ditto)
#
# msdos_dshd_5.25 - MS-DOS 5.25"DSHD 1.2MB
#?msdos_dshd_5.25_1394 - 5.25"DSHD 'FDFORMAT' format, 82 trk ? sector ? byte/sect 1394k
#?msdos_dshd_5.25_2m_1476 - 5.25"DSHD '2M/F' format, 82 trk ? sector ?byte/sect 1476k
#?msdos_dshd_5.25_2m_1558 - 5.25"DSHD '2M/M' format, 82 trk ? sector ?byte/sect 1558k
#?msdos_dshd_5.25_2m_1639 - 5.25"DSHD '2MGUI' format, 82 trk ? sector ?byte/sect 1639k
#
# msdos_dsdd - MS-DOS 3.5"DD 80 trk 9 sector 720K
#\atari_dsdd - ditto, same format
#?msdos_dsdd_820     - 3.5"DD 'FDFORMAT' format, 82 trk ? sector ? byte/sect 820k
#?msdos_dsdd_2m_984  - 3.5"DD '2M/F' format, 82 trk ? sector ?byte/sect 984k
#?msdos_dsdd_2m_1066 - 3.5"DD '2M/M' format, 82 trk ? sector ?byte/sect 1066k
#?msdos_dsdd_2m_1176 - 3.5"DD '2MGUI' format, 82 trk ? sector ?byte/sect 1176k
#
# msdos_dshd - MS-DOS 3.5"HD 80 trk 18 sector 512 byte/sect 1440K
#\atari_dshd - ditto, same format
#\mac_dshd   - ditto, same format
# msdos_dshd_1476 - 3.5"HD '?' format, 82 trk 18 sector 512 byte/sect 1476K
# msdos_dshd_1494 - 3.5"HD '?' format, 83 trk 18 sector 512 byte/sect 1494K 
# msdos_dshd_1600 - 3.5"HD '?' format, 80 trk 20 sector 512 byte/sect 1600K
# msdos_dshd_1680 - 3.5"HD 'DMF' format, 80 trk 21 sector 512 byte/sect 1680K
# msdos_dshd_1722 - 3.5"HD 'FDFORMAT' format, 82 trk 21 sector 512 byte/sect 1722K
# msdos_dshd_1743 - 3.5"HD '?' format, 83 trk 21 sector 512 byte/sect 1743K
# msdos_dshd_1760 - 3.5"HD '?' format, 80 trk 11 sector 1024 byte/sect 1743K
# msdos_dshd_1802 - 3.5"HD '?' format, 82 trk 11-19 sectors, 1024 byte/sect 1802K
#?msdos_dshd_1860 - IBM OS/2 3.5"HD 'XDF' format, 1860k, variable sector size per track
#?msdos_dshd_2m_1804 - 3.5"HD '2M/F' format, 82 trk ? sector ?byte/sect 1804k
#?msdos_dshd_2m_1886 - 3.5"HD '2M/M' format, 82 trk ? sector ?byte/sect 1886k
#?msdos_dshd_2m_1972 - 3.5"HD '2MGUI' format, 82 trk ? sector ?byte/sect 1972k
#
#?msdos_dsed - MS-DOS 3.5"ED 80 trk ?36? sector 2880K
#?msdos_dsed_2m_3608 - 3.5"ED '2M/F' format, 82 trk ? sector ?byte/sect 3608k
#?msdos_dsed_2m_3772 - 3.5"ED '2M/M' format, 82 trk ? sector ?byte/sect 3772k
#?msdos_dsed_2m_3944 - 3.5"ED '2MGUI' format, 82 trk ? sector ?byte/sect 3944k
#?cpm_sssd_5.25 - FM modulated, 10 sectors, otherwise the same as msdos_ssdd_5.25
#?cpm_dssd_5.25 - FM modulated, 10 sectors, otherwise the same as msdos_dsdd_5.25
#
#
# ***APPLE/MACINTOSH***
#
# mac_5.25
# mac_5.25_300
# mac_dsdd_800
#\mac_dsdd
# mac_dsdd_800_524
#\mac_dsdd_524
#
#
# ***VARIOUS OTHERS***
#
# ibm_dsdd      - IBM 8" floppy, FM modulation on T0S0 otherwise MFM
# dec_rx01_sssd - DEC RX01 8" cartridge/floppy, FM modulation
# dg_dsdd       - Digital Group 8" floppy, MFM, 77 trks, 2 sides, 15 sec/trk
# victor9000_ssdd
# victor9000_dsdd

options
	{
	always_initialize yes	# always initialize drives with cwtool -R and
				# and cwtool -W
	}

disk "clear"
	{
	info "RAW, 14 MHz, 83 trks, 2 sides, CLEARS ALL TRACKS OF A DISK"
	format "fill"
	clock 14
	timeout 230
	track_range 0 165 1 optional yes
	}

disk "pattern_14"
	{
	info "RAW, 14 MHz, 83 trks, 2 sides, CLEARS ALL TRACKS OF A DISK"
	format "fill"
	clock 14
	timeout 420
	indexed yes
	write mode 1
	track_range 0 165 1 optional yes
	}

