disk "c1541_g64"
	{
	info "C1541, RAW GCR, 5.25 inch, 35 trks, 1 side"
	side_offset 84
	track_range 1 69 2
		{
		format "fill"
		optional yes
		write fill_length 16384
		}
	format "gcr_g64"
	image "g64"
	clock 14
	strip_track yes
	read
		{
		timeout 180
		align_track yes
		postcomp_simple yes
		sync_length 10
		}
	write
		{
		timeout 300
		precomp3
			{
			+0x0000 +0x0100 +0x0100
			-0x0100 +0x0000 +0x0000
			-0x0100 +0x0000 +0x0000
			}
		precomp2
			{
			+0x0000 +0x0100 +0x0100
			-0x0100 +0x0000 +0x0000
			-0x0100 +0x0000 +0x0000
			}
		precomp1
			{
			+0x0000 +0x0100 +0x0100
			-0x0100 +0x0000 +0x0000
			-0x0100 +0x0000 +0x0000
			}
		precomp0
			{
			+0x0000 +0x0100 +0x0100
			-0x0100 +0x0000 +0x0000
			-0x0100 +0x0000 +0x0000
			}
		prolog_length 2048
		}
	track_range 0 32 2 read speed 3
	track_range 34 46 2 read speed 2
	track_range 48 58 2 read speed 1
	track_range 60 68 2 read speed 0
	}

